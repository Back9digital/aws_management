{   
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "app/**/*.js",
      "lib/**/*.js"
    ],
    "exclude": [
      "./test/*"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": false,
    "report-dir": "./coverage",
    "cache": false
  }